OBJS = rugged.o rugged_backend.o rugged_blame.o rugged_blob.o rugged_branch.o rugged_branch_collection.o rugged_commit.o rugged_config.o rugged_cred.o rugged_diff.o rugged_diff_delta.o rugged_diff_hunk.o rugged_diff_line.o rugged_index.o rugged_note.o rugged_object.o rugged_patch.o rugged_rebase.o rugged_reference.o rugged_reference_collection.o rugged_remote.o rugged_remote_collection.o rugged_repo.o rugged_revwalk.o rugged_settings.o rugged_signature.o rugged_submodule.o rugged_submodule_collection.o rugged_tag.o rugged_tag_collection.o rugged_tree.o
