ORIG_SRCS = rugged.c rugged_backend.c rugged_blame.c rugged_blob.c rugged_branch.c rugged_branch_collection.c rugged_commit.c rugged_config.c rugged_cred.c rugged_diff.c rugged_diff_delta.c rugged_diff_hunk.c rugged_diff_line.c rugged_index.c rugged_note.c rugged_object.c rugged_patch.c rugged_rebase.c rugged_reference.c rugged_reference_collection.c rugged_remote.c rugged_remote_collection.c rugged_repo.c rugged_revwalk.c rugged_settings.c rugged_signature.c rugged_submodule.c rugged_submodule_collection.c rugged_tag.c rugged_tag_collection.c rugged_tree.c
