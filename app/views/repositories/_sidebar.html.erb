<div class="col-sm-3 col-md-2 sidebar">
  <ul class="nav nav-sidebar">
    <h3> Repositories in <%= @organization.name %></h3>
    <% if !@repos.empty? %>
      <% @repos.each do |repo| %>
          <li><%=link_to repo.name, show_vulnerability_path(repo.id, :organization_id => @organization.id) %></li>
      <% end %>
    <% end %>
  </ul>
</div>